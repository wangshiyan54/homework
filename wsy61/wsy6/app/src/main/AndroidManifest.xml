<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wsy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.wsy.TextReadActivity"
            android:exported="true" />
        <activity
            android:name="com.example.wsy.TextWriteActivity"
            android:exported="true" />
        <activity
            android:name="com.example.wsy.ImageReadActivity"
            android:exported="true" />
        <activity
            android:name="com.example.wsy.ImageWriteActivity"
            android:exported="true" />
        <activity android:name="com.example.wsy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>